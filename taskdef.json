{
    "executionRoleArn": "arn:aws:iam::163622408598:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ],
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "name": "container-app-python",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/family-entrega3-devops",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "secrets": [
                {
                    "name": "DB_USER",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:163622408598:secret:entrega3-secrets-MeDBlf"
                },
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:163622408598:secret:entrega3-secrets-MeDBlf"
                },                
                {
                    "name": "DB_HOST_DOCKER",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:163622408598:secret:entrega3-secrets-MeDBlf"
                },                
                {
                    "name": "DB_PORT",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:163622408598:secret:entrega3-secrets-MeDBlf"
                },                
                {
                    "name": "DB_NAME",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:163622408598:secret:entrega3-secrets-MeDBlf"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "family-entrega3-devops"
}